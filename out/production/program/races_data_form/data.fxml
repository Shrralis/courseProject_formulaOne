<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ScrollPane fitToHeight="true" hbarPolicy="NEVER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="250.0" prefWidth="272.0" vbarPolicy="NEVER" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="races_data_form.Controller">
   <GridPane hgap="8.0" prefHeight="250.0" prefWidth="300.0">
      <TextField fx:id="year" promptText="Рік" />
      <TextField fx:id="country" promptText="Країна" GridPane.rowIndex="1" />
      <TextField fx:id="city" promptText="Місто" GridPane.rowIndex="2" />
      <TextField fx:id="track_name" promptText="Назва траси" GridPane.rowIndex="3" />
      <TextField fx:id="laps" promptText="Кількість кругів" GridPane.rowIndex="4" />
      <TextField fx:id="lap_length" promptText="Довжина круга" GridPane.rowIndex="5" />
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="16.0" GridPane.rowIndex="6">
         <Button mnemonicParsing="false" onMouseClicked="#onMouseCancelClick" text="Відміна" />
         <Button mnemonicParsing="false" onMouseClicked="#onMouseOkClick" text="ОК" />
         <padding>
            <Insets bottom="8.0" left="8.0" top="8.0" />
         </padding>
      </HBox>
      <CheckBox fx:id="bYear" mnemonicParsing="false" onAction="#onCheckBoxEnablerSelect" GridPane.columnIndex="1" />
      <CheckBox fx:id="bCountry" mnemonicParsing="false" onAction="#onCheckBoxEnablerSelect" GridPane.columnIndex="1"
                GridPane.rowIndex="1" />
      <CheckBox fx:id="bCity" mnemonicParsing="false" onAction="#onCheckBoxEnablerSelect" GridPane.columnIndex="1"
                GridPane.rowIndex="2" />
      <CheckBox fx:id="bTrack_name" mnemonicParsing="false" onAction="#onCheckBoxEnablerSelect" GridPane.columnIndex="1"
                GridPane.rowIndex="3" />
      <CheckBox fx:id="bLaps" mnemonicParsing="false" onAction="#onCheckBoxEnablerSelect" GridPane.columnIndex="1"
                GridPane.rowIndex="4" />
      <CheckBox fx:id="bLap_length" mnemonicParsing="false" onAction="#onCheckBoxEnablerSelect" GridPane.columnIndex="1"
                GridPane.rowIndex="5" />
      <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="274.0" minWidth="-Infinity" prefWidth="251.0" />
         <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="16.0" />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <padding>
         <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
      </padding>
   </GridPane>
</ScrollPane>
