<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox fx:controller="results_data_form.Controller" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="250.0" prefWidth="300.0" spacing="8.0" xmlns="http://javafx.com/javafx/8.0.111"
      xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
   <children>
      <ComboBox fx:id="race" prefHeight="27.0" prefWidth="282.0" promptText="Оберіть гонку" />
      <ComboBox fx:id="pilot" prefHeight="27.0" prefWidth="282.0" promptText="Оберіть пілота" />
      <ComboBox fx:id="team" prefHeight="27.0" prefWidth="282.0" promptText="Оберіть команду" />
      <TextField fx:id="raceTime" promptText="Час гонки" />
      <TextField fx:id="pilotPoints" promptText="Очки пілота" />
      <TextField fx:id="teamPoints" promptText="Очки команди" />
      <HBox alignment="CENTER_RIGHT" spacing="16.0">
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" />
         </padding>
         <children>
            <Button mnemonicParsing="false" text="Відміна" onMouseClicked="#onMouseCancelClick" />
            <Button mnemonicParsing="false" text="ОК" onMouseClicked="#onMouseOkClick" />
         </children>
      </HBox>
   </children>
</VBox>
