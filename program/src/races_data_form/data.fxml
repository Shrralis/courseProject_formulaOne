<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox fx:controller="races_data_form.Controller" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="250.0" prefWidth="300.0" spacing="8.0" xmlns="http://javafx.com/javafx/8.0.111"
      xmlns:fx="http://javafx.com/fxml/1">
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
   <children>
      <TextField fx:id="year" promptText="Рік" />
      <TextField fx:id="country" promptText="Країна" />
      <TextField fx:id="city" promptText="Місто" />
      <TextField fx:id="trackName" promptText="Назва траси" />
      <TextField fx:id="laps" promptText="Кількість кругів" />
      <TextField fx:id="lapLength" promptText="Довжина круга" />
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="16.0">
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" />
         </padding>
         <children>
            <Button mnemonicParsing="false" text="Відміна" onMouseClicked="#onMouseCancelClick" />
            <Button mnemonicParsing="false" text="ОК" onMouseClicked="#onMouseOkClick" />
         </children>
      </HBox>
   </children>
</VBox>
